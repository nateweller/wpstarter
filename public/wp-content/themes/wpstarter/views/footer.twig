        <footer class="site-footer">
            <nav aria-labelledby="footer-menu-label">
                <h2 id="footer-menu-label" class="hidden">Site Menu</h2>
                <ul class="menu footer-menu">
                    {% for menuItem in menu %}
                        <li class="menu-item {{ item.object_id == post.id ? 'current-page' }} {{ menuItem.classes|join }}">
                            {% if menuItem.object_id == post.id %}
                                <span class="hidden">Current Page:</span> {{ menuItem.title }}
                            {% else %}
                                <a href="{{ menuItem.url }}">{{ menuItem.title }}</a>
                            {% endif %}
                        </li>
                    {% endfor %}
                </ul>
            </nav>
        </footer>
        {{ wp_footer }}
        {{ debugger }}
    </body>
</html>